<main id="contact">
        <section>
            <h2 data-translate="contact_title">{{main.contact_title|upper}}</h2>
            <p data-translate="contact_p">{{main.contact_p}}</p>
            <form action="#" method="POST">
                <input type="text" id="identity" name="identity" placeholder="{{main.contact_name_pl}}" maxlength="64"
                    required="required" />
                <label for="identity">{{main.contact_name_l|upper}}</label>
                <input type="text" id="object" name="object" placeholder="{{main.contact_subject_pl}}" maxlength="128"
                    required="required" />
                <label for="object">{{main.contact_subject_l|upper}}</label>
                <textarea id="message" name="message" placeholder="{{main.contact_message_pl}}" rows="10" cols="33"
                    maxlength="1024" required="required"></textarea>
                <label for="message">{{main.contact_message_l|upper}}</label>
                <input type="submit" name="submit" id="submit" value="{{main.contact_message_submit}}" class="hidden">
                <label for="submit" class="button">{{main.contact_message_submit}}</label>
            </form>
        </section>
        <section>
            <h2 data-translate="contact_mail">{{main.contact_mail|upper}}</h2>
            <a href="mailto:{{main.contact_mailto}}">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                    <title>{{main.contact_mail_title}}</title>
                    <path
                        d="M24 0l-6 22-8.129-7.239 7.802-8.234-10.458 7.227-7.215-1.754 24-12zm-15 16.668v7.332l3.258-4.431-3.258-2.901z" />
                </svg>
                {{main.contact_mailto}}
            </a>
            <p data-translate="contact_p">{{main.contact_p}}</p>
        </section>
        <section>
            <h2 data-translate="contact_sm_title">{{main.contact_sm_title|upper}}</h2>
            <ul>
